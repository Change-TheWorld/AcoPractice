{
  "globals": {
    "jasmine": true,
    "angular": true,
    "browser": true,
    "element": true,
    "window":true,
    "define": true
  },
  "strict":true,  //严格模式校验
  "camelcase":true,  //使用驼峰命名(camelCase)或全大写下划线命名(UPPER_CASE)
  "unused":false,  //未使用变量
  "undef":true,  //未定义变量名
  "eqnull":true,  //==null通常用来比较=== null || === undefined
  "curly": true,//表示所有的代码块必须使用大括号
  "eqeqeq": true,//表示判断相等时，必须使用 ===
  "immed": true,//表示立即执行函数必须用括号包起来 (function () { } ());
  "noarg": true,//表示禁止使用 arguments.caller 和 arguments.callee
  "noempty": true,//表示禁止出现空的代码块 { }
  "quotmark": "false",//单双引号
  "debug":false,    //严格模式校验
  "bitwise": true,
  "es3": false,
  "forin": true,
  "freeze": true,
  "indent": 4,
  "latedef": "nofunc",
  "newcap": true,
  "nonbsp": true,
  "nonew": true,
  "plusplus": false,
  "maxparams": 10,
  "maxdepth": 5,
  "maxstatements": 40,
  "maxcomplexity": 8,
  "maxlen": 120,

  "asi": false,
  "boss": false,
  "esnext": false,
  "evil": false,
  "expr": false,
  "funcscope": false,
  "globalstrict": false,
  "iterator": false,
  "lastsemic": false,
  "laxbreak": false,
  "laxcomma": false,
  "loopfunc": true,
  "maxerr": false,
  "moz": false,
  "multistr": false,
  "notypeof": false,
  "proto": false,
  "scripturl": false,
  "shadow": false,
  "sub": true,
  "supernew": false,
  "validthis": false,
  "noyield": false,

  "browser": true,
  "node": true
}
